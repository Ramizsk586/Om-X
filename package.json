{
  "name": "om-x",
  "productName": "Om-X",
  "version": "2.0.0",
  "description": "Om-X - AI Browser with Minecraft Server Integration",
  "main": "main.js",
  "author": "Sk Abdul Ramiz",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ramizsk586/Om-X.git"
  },
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "rebuild": "electron-rebuild"
  },
  "workspaces": [
    "tools/ddg"
  ],
  "dependencies": {
    "@ghostery/adblocker-electron": "^2.14.1",
    "@google/genai": "^1.34.0",
    "@lmstudio/sdk": "^1.5.0",
    "axios": "^1.6.0",
    "chess.js": "^1.0.0",
    "cross-fetch": "^4.1.0",
    "dotenv": "^16.4.5",
    "electron-updater": "^6.1.7",
    "express": "^4.18.2",
    "groq-sdk": "^0.3.3",
    "ip": "^2.0.0",
    "jspdf": "^2.5.1",
    "minecraft-data": "^3.70.0",
    "mineflayer": "^4.34.0",
    "mineflayer-collectblock": "^1.6.0",
    "mineflayer-pathfinder": "^2.4.5",
    "node-fetch": "^3.3.2",
    "prismarine-viewer": "^1.33.0",
    "sarvamai": "^1.1.4",
    "socket.io": "^4.7.0",
    "uuid": "^9.0.0",
    "yt-dlp-wrap": "^2.3.12",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.8",
    "electron": "^33.0.0",
    "electron-builder": "^25.1.8",
    "electron-rebuild": "^3.2.9"
  },
  "build": {
    "appId": "com.omx.browser",
    "productName": "Om-X.browser",
    "protocols": [
      {
        "name": "HTTP Protocol",
        "schemes": [
          "http",
          "https"
        ]
      }
    ],
    "fileAssociations": [
      {
        "ext": [
          "html",
          "htm",
          "shtml",
          "xhtml"
        ],
        "name": "HTML Document",
        "role": "Viewer",
        "mimeType": "text/html"
      }
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "Ramizsk586",
        "repo": "Om-X"
      }
    ],
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!dist/*",
      "!**/*.ts",
      "!**/*.map"
    ],
    "asarUnpack": [
      "game/electron/mindclone/**"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icons/app.ico",
      "legalTrademarks": "Om-X Browser"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Om-X",
      "deleteAppDataOnUninstall": true,
      "perMachine": false
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icons/app.icns",
      "category": "public.app-category.productivity",
      "extendInfo": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "Web URL",
            "CFBundleURLSchemes": [
              "http",
              "https"
            ]
          }
        ]
      }
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/icons/icon.png",
      "category": "WebBrowser",
      "mimeTypes": [
        "text/html",
        "x-scheme-handler/http",
        "x-scheme-handler/https"
      ]
    }
  }
}
